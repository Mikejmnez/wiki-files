            &lt;/Attribute&gt;
            &lt;Attribute name="units" type="String"&gt;
                &lt;value&gt;m s-1&lt;/value&gt;
            &lt;/Attribute&gt;
            &lt;Attribute name="_FillValue" type="Int16"&gt;
                &lt;value&gt;-32768&lt;/value&gt;
            &lt;/Attribute&gt;
